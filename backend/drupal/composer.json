{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
              "name": "jquery/select2",
              "version": "4.0.5",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/select2/select2/archive/4.0.5.zip",
                "type": "zip"
              }
            }
        },
        {
            "type": "package",
            "package": {
              "name": "heiseonline/shariff",
              "version": "3.1.1",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/heiseonline/shariff/releases/download/3.1.1/shariff-3.1.1.zip",
                  "type": "zip"
              }
            }
        }
    ],
    "require": {
        "commerceguys/addressing": "^1.0",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.4",
        "drupal/chosen": "^2.6",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.6.0",
        "drupal/devel": "^2.0",
        "drupal/field_group": "^3.0",
        "drupal/fieldable_path": "^1.0@RC",
        "drupal/geocoder": "^2.0",
        "drupal/geofield": "^1.0@beta",
        "drupal/json_field": "^1.0@RC",
        "drupal/jsonapi": "^2.4",
        "drupal/jsonapi_extras": "^3.5",
        "drupal/linkit": "^4.3",
        "drupal/redirect": "^1.3",
        "drupal/restui": "^1.16",
        "drupal/route_condition": "^1.0",
        "drupal/switch_page_theme": "^1.0",
        "drupal/webform": "^5.1",
        "drupal/webform_views": "^5.0",
        "drush/drush": "^9.0.0",
        "heiseonline/shariff": "^3.1",
        "jquery/select2": "^4.0",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.6.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
